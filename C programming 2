#define _CRT_SECURE_NO_WARNINGS

#include <stdio.h>
#include <string.h>

int main(void)
{
	char string[30];

	printf("문자 입력 : ");

	scanf("%s", string);


	for (int i = 0; i < strlen(string); i++)
	{
		if (string[i] < 0)
		{
			printf("한");
			i++;
		}
		else
		{
			printf("영");
		}
	}

	printf("\n%s", string);

	return 0;
}
